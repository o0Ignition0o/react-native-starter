{
  "name": "react_native_starter_kit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "test": "jest",
    "lint": "eslint ./ --cache --fix --ignore-path .gitignore",
    "sync-locales": "node scripts/locales.js",
    "add-image": "node scripts/images.js",
    "clean-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules && yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "jsc-android": "236355.1.1",
    "lottie-react-native": "2.5.11",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-config": "0.11.7",
    "react-native-gesture-handler": "1.0.12",
    "react-native-screens": "1.0.0-alpha.22",
    "react-navigation": "3.0.9",
    "styled-components": "4.1.3",
    "styled-system": "3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-module-resolver": "3.1.2",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "2.4.2",
    "csvtojson": "2.0.8",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.5.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-import-resolver-react-native": "0.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "fs": "0.0.1-security",
    "husky": "1.3.1",
    "imageoptim": "0.5.0",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "metro-react-native-babel-preset": "0.51.1",
    "ora": "3.0.0",
    "path": "0.12.7",
    "prettier": "1.15.3",
    "react-test-renderer": "16.6.3",
    "request": "2.88.0",
    "sharp": "0.21.3",
    "util": "0.11.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
