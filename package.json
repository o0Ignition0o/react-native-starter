{
  "name": "react_native_starter_kit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "test": "jest",
    "lint": "eslint ./ --cache --fix --ignore-path .gitignore",
    "clean-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules && yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "react": "16.6.1",
    "react-native": "0.57.5",
    "react-native-gesture-handler": "1.0.10",
    "react-native-screens": "1.0.0-alpha.17",
    "react-navigation": "3.0.6"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-import-resolver-react-native": "0.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "metro-react-native-babel-preset": "0.49.2",
    "prettier": "1.15.2",
    "react-test-renderer": "16.6.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
